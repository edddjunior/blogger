<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", login_path, :class =>"text-dark" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Não tenho uma conta", new_registration_path(resource_name), :class =>"text-dark" %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Esqueceu sua senha", new_password_path(resource_name), :class =>"text-dark" %><br />
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Não recebi as instruções de confirmação", new_confirmation_path(resource_name), :class =>"text-dark" %><br />
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Não recebi as instruções de desbloqueio", new_unlock_path(resource_name), :class =>"text-dark" %><br />
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider, :class =>"text-dark") %><br />
  <% end %>
<% end %>
